{
  "name": "contentful-to-sanity",
  "version": "3.0.0",
  "description": "Migration tool from Contentful to Sanity",
  "keywords": [
    "sanity",
    "contentful",
    "migration"
  ],
  "homepage": "https://github.com/sanity-io/contentful-to-sanity",
  "author": "Sanity.io <hello@sanity.io>",




  "bugs": {
    "url": "https://github.com/sanity-io/contentful-to-sanity/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/contentful-to-sanity.git"  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "source": "./src/cli/index.ts",
      "require": "./dist/cli.cjs",
      "default": "./dist/cli.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "bin": {
    "contentful-to-sanity": "./bin/contentful-to-sanity.cjs"
  },
  "files": [
    "bin",
    "dist",
    "CHANGELOG.md"
  ],
  "browserslist": [
    "> 0.2% and supports es6-module and supports es6-module-dynamic-import and not dead and not IE 11",
    "maintained node versions"
  ],
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "cli": [
        "./dist/cli.d.ts"
      ]
    }
  },


  "scripts": {
    "build": "tsup",
    "format": "prettier --cache --ignore-path .gitignore --write .",
    "lint": "eslint . --cache",
    "lint:fix": "npm run lint -- --fix",
    "test": "vitest"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": false,
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@portabletext/contentful-rich-text-to-portable-text": "^0.1.0",
    "@sanity/block-tools": "^3.6.0",
    "@sanity/client": "^5.2.2",
    "@sanity/schema": "^3.6.0",
    "axios": "^0.25.0",
    "case": "^1.6.3",
    "configstore": "^6.0.0",
    "contentful": "^9.3.3",
    "contentful-export": "^7.19.36",
    "contentful-management": "^10.30.0",
    "execa": "^6.0.0",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.2.0",
    "javascript-stringify": "^2.1.0",
    "joi": "^17.5.0",
    "jsdom": "^19.0.0",
    "just-compact": "^3.0.1",
    "just-omit": "^2.0.1",
    "just-safe-get": "^4.0.1",
    "just-safe-set": "^4.0.2",
    "listr": "^0.14.3",
    "markdown-it": "^12.3.2",
    "soly": "^1.1.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@sanity/portable-text-editor": "^3.6.0",
    "@sanity/types": "^3.6.0",
    "@types/configstore": "^5.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.1.3",
    "@types/jsdom": "^16.2.14",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitest/coverage-c8": "^0.29.2",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "globby": "^11.1.0",
    "moment": "^2.29.1",
    "prettier": "^2.8.4",
    "prettier-plugin-packagejson": "^2.4.3",
    "shx": "^0.3.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "tslib": "^2.3.1",
    "tsup": "^5.11.13",
    "typescript": "^4.9.5",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.29.2",
    "vitest-github-actions-reporter": "^0.10.0"
  },
  "engines": {
    "node": ">=14.18"
  }
}
