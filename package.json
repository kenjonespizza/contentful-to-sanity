{
  "name": "contentful-to-sanity",
  "version": "3.0.0",
  "description": "Migration tool from Contentful to Sanity",
  "keywords": [
    "sanity",
    "contentful",
    "migration"
  ],
  "homepage": "https://github.com/sanity-io/contentful-to-sanity",
  "bugs": "https://github.com/sanity-io/contentful-to-sanity/issues",
  "repository": "sanity-io/contentful-to-sanity",
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "contentful-to-sanity": "./bin/run"
  },
  "files": [
    "/bin",
    "/dist",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "format": "prettier --cache --ignore-path .gitignore --write .",
    "lint": "eslint . --cache",
    "lint:fix": "npm run lint -- --fix",
    "test": "vitest"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": false,
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@portabletext/contentful-rich-text-to-portable-text": "^0.1.0",
    "@sanity/block-tools": "^2.27.0",
    "@sanity/client": "^3.0.3",
    "@sanity/schema": "^2.27.0",
    "axios": "^0.25.0",
    "case": "^1.6.3",
    "configstore": "^6.0.0",
    "contentful": "^9.1.7",
    "contentful-export": "^7.14.22",
    "contentful-management": "^7.53.0",
    "execa": "^6.0.0",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.2.0",
    "javascript-stringify": "^2.1.0",
    "joi": "^17.5.0",
    "jsdom": "^19.0.0",
    "just-compact": "^3.0.1",
    "just-omit": "^2.0.1",
    "just-safe-get": "^4.0.1",
    "just-safe-set": "^4.0.2",
    "markdown-it": "^12.3.2",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@sanity/portable-text-editor": "^2.26.0",
    "@sanity/types": "^2.26.0",
    "@types/chai": "^4",
    "@types/configstore": "^5.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.1.3",
    "@types/jsdom": "^16.2.14",
    "@types/markdown-it": "^12.2.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitest/coverage-c8": "^0.29.2",
    "chai": "^4",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "globby": "^11.1.0",
    "mocha": "^9",
    "moment": "^2.29.1",
    "prettier": "^2.8.4",
    "prettier-plugin-packagejson": "^2.4.3",
    "react": "^17.0.2",
    "shx": "^0.3.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "tslib": "^2.3.1",
    "tsup": "^5.11.13",
    "typescript": "^4.9.5",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.29.2",
    "vitest-github-actions-reporter": "^0.10.0"
  },
  "engines": {
    "node": ">=14.18"
  }
}
